<h1>Listado de Usuarios</h1>

<div class="well">
  <table class="table table-striped table-hover ">
    <thead>
      <tr>
        <th>Name</th>
        <th>Mail</th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.mail %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
